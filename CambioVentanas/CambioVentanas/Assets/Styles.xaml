<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <FontFamily x:Key="Gotham">pack://application:,,,/Assets/fonts/#HomepageBaukasten Book</FontFamily>
    <FontFamily x:Key="Neitherly">pack://application:,,,/Assets/fonts/#Neitherly Demo Light</FontFamily>
    <FontFamily x:Key="NeitherlyScript">pack://application:,,,/Assets/fonts/#Neitherly Demo Script</FontFamily>

    <SolidColorBrush x:Key="BackgroundColorWindow1" Color="#121212" />
    <SolidColorBrush x:Key="BackgroundColorWindow2" Color="#3C3C3C" />
    <SolidColorBrush x:Key="FontColorBasic" Color="White" />
    <SolidColorBrush x:Key="BackgroundColorMedioAmarillo" Color="#f4db04" />

    <Style TargetType="Button">
        <Setter Property="Width" Value="16" />
        <Setter Property="Height" Value="16" />
        <Setter Property="Margin" Value="5" />
    </Style>


</ResourceDictionary>